{
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=86400, immutable"
      },
      "continue": true
    },
    {
      "src": "/games/:path*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "games/**/*.{unityweb,asm,mem}",
      "headers": {
        "Content-Encoding": null
      },
      "continue": true
    }
  ],
  "rewrites": [
    {
      "source": "/games/:path*",
      "destination": "/games/:path*",
      "headers": [
        {
          "key": "Content-Encoding",
          "value": "gzip"
        }
      ]
    }
  ],
  "builds": [
    {
      "src": "games/**/*.{html,js,css,png,jpg,jpeg,gif,json}",
      "use": "@vercel/static"
    },
    {
      "src": "games/**/*.{unityweb,asm,mem}",
      "use": "@vercel/static-build"
    },
    {
      "src": "*.gz",
      "use": "@vercel/static"
    }
  ]
}