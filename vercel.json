{
    "git": {
        "deploymentEnabled": {
            "main": true
        }
    },
    "builds": [
        { "src": "**/*.+(unityweb|gz)", "use": "@vercel/static-build" }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "headers": {
            "Cache-Control": "public, max-age=31536000, immutable"
          },
          "continue": true
        }
      ]
}