{
    "git": {
        "deploymentEnabled": {
            "main": true
        }
    },
    "builds": [
      { "src": "**/*.+(unityweb|gz)", "use": "@vercel/static-build" }
    ],
    "routes": [
      {
        "src": "/.*\\.(unityweb)$",
        "headers": {
          "Content-Encoding": null
        },
        "continue": true
      },
      {
        "src": "/(.*)",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        },
        "continue": true
      }
    ]
}